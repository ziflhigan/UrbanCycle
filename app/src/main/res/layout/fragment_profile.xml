<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Profile.ProfileFragment">
    <RelativeLayout
        android:id="@+id/BgRegister"
        android:layout_marginTop="55dp"
        android:layout_width="match_parent"
        android:layout_height="420dp"
        android:background="@drawable/bg_waves"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <View
        android:layout_width="match_parent"
        android:layout_height="?android:attr/actionBarSize"
        android:visibility="invisible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="113dp"
        android:layout_height="106dp"
        android:layout_marginTop="56dp"
        android:layout_marginEnd="296dp"
        android:background="@android:color/transparent"
        android:src="@drawable/profi"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="100dp"
        android:layout_height="69dp"
        android:layout_marginStart="308dp"
        android:layout_marginTop="772dp"
        android:src="@drawable/brand"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/Welcom"
        android:layout_width="195dp"
        android:layout_height="97dp"
        android:layout_marginTop="56dp"
        android:layout_marginEnd="16dp"
        android:fontFamily="cursive"
        android:text="Welcom to UrbanCycle"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/UsernameDisp"
        android:layout_width="72dp"
        android:layout_height="36dp"
        android:layout_marginTop="84dp"
        android:layout_marginEnd="188dp"
        android:fontFamily="casual"
        android:text="Name "
        android:textAppearance="@style/TextAppearance.AppCompat.Display2"
        android:textSize="30sp"
        android:textStyle="bold|italic"
        android:typeface="sans"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/History"
        android:layout_width="116dp"
        android:layout_height="117dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="380dp"
        android:background="@android:color/transparent"
        android:contentDescription="Saving history"
        android:src="@drawable/history"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="36dp"
        android:text="History"
        android:textColor="@android:color/holo_purple"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/History" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="66dp"
        android:layout_height="25dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="164dp"
        android:text="Setting"
        android:textColor="@android:color/holo_purple"
        android:textSize="20sp"
        android:textStyle="italic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Setting" />

    <ImageButton
        android:id="@+id/Setting"
        android:layout_width="120dp"
        android:layout_height="128dp"
        android:layout_marginTop="420dp"
        android:layout_marginEnd="140dp"
        android:background="@android:color/transparent"
        android:contentDescription="Setting"
        android:src="@drawable/set"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/Preference"
        android:layout_width="319dp"
        android:layout_height="297dp"
        android:layout_marginEnd="168dp"
        android:layout_marginBottom="292dp"
        android:src="@drawable/prefe"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="152dp"
        android:text="Your prefer"
        android:textColor="@android:color/holo_purple"
        android:textSize="20sp"
        android:textStyle="italic"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView3" />

</androidx.constraintlayout.widget.ConstraintLayout>